{
  "name": "@project-arcturus/web",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "www"
  ],
  "scripts": {
    "prettier": "prettier --write www",
    "start": "serve www",
    "dev": "browser-sync www -w --port 4200",
    "sync:css": "rm -rf www/css/design && mkdir -p www/css/design && cp -R node_modules/@project-arcturus/design-tokens/build/css/*.css www/css/design/",
    "sync:ws-vendor": "cp -R node_modules/@project-arcturus/store/dist/esm/bundle.mjs www/js/vendor/@project-arcturus/store.js"
  },
  "dependencies": {
    "@project-arcturus/design-tokens": "workspace:*",
    "@project-arcturus/store": "workspace:*",
    "front-matter": "^4.0.2",
    "lottie-web": "^5.12.2",
    "lru-cache": "^11.0.0",
    "mitt": "^3.0.1",
    "serve": "^14.2.3",
    "sleepydogs": "^1.0.7",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "browser-sync": "^3.0.3",
    "eslint": "^9.12.0",
    "globals": "^15.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3"
  }
}
